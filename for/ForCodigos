for c in range(9, 0,-2):
    print(c)
print('fim')    



n=int(input('insira um numero'))
for c in range(0,n):
    print(c)
print('fim')    


i=int(input('inicio: '))
f=int(input('fim: '))
p=int(input('de quanto em quanto +- '))
for c in range(i ,f+1, p):
    print(c)
print('fim')    


s=0
for c in range(0,4):
    n=int (input('digite um valor: '))
    s= s + n

print(f'A soma dos valores Ã© {s}')    